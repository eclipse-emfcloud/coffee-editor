{
  "name": "coffee-comparison",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.7.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@eclipsesource/comparison-extension": "0.0.1-next.20220414-105416",
