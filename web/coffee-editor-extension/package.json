{
  "name": "coffee-editor-extension",
  "license": "(EPL-2.0 OR MIT)",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.7.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@eclipse-emfcloud/modelserver-theia": "next",
