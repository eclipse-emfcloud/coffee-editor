{
  "name": "coffee-editor-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "1.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@jsonforms/core": "2.3.0",
