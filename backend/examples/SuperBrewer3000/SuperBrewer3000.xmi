<?xml version="1.0" encoding="UTF-8"?>
<com.eclipsesource.coffee.model:Machine
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:com.eclipsesource.coffee.model="http://www.eclipsesource.com/modelserver/example/coffeemodel"
    name="SuperBrewer3000">
  <children
      xsi:type="com.eclipsesource.coffee.model:BrewingUnit">
    <children
        xsi:type="com.eclipsesource.coffee.model:ControlUnit">
      <processor
          vendor="Qualcomm"
          clockSpeed="5"
          numberOfCores="10"
          socketconnectorType="Z51"
          thermalDesignPower="1000"/>
      <dimension
          width="10"
          height="12"
          length="13"/>
      <display
          width="10"
          height="20"/>
    </children>
  </children>
  <workflows>
    <nodes xsi:type="com.eclipsesource.coffee.model:AutomaticTask"
        name="Preheat"/>
    <nodes xsi:type="com.eclipsesource.coffee.model:AutomaticTask"
        name="Brew"/>
  </workflows>
</com.eclipsesource.coffee.model:Machine>
