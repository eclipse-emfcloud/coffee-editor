<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.eclipsesource.coffee.modelserver.app.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.eclipsesource.coffee.modelserver.app.application"
            name="Coffee Model Server">
         <property
               name="appName"
               value="Coffee Model Server">
         </property>
      </product>
   </extension>
</plugin>
