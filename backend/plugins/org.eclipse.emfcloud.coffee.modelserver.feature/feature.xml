<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.emfcloud.coffee.modelserver.feature"
      label="Coffee Modelserver Feature"
      version="0.1.0.qualifier"
      provider-name="EclipseSource">

